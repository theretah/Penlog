@model Penlog.ViewModels.ProfileImageVM
@{
    string Url = Model.ProfileImageDataUrl;
    string preText = (Url == "default-profile.jpg") ? "~/images/" : "";
}
<img width="@Model.ImageWidthAndHeight" height="@Model.ImageWidthAndHeight" class="border border-dark rounded-circle"
     src="@(preText + Url)" />
